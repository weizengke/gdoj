			<interceptor-ref name="isLogin"></interceptor-ref>	
			<interceptor-ref name="isAdmin"></interceptor-ref>			
		</action>
		
		<action name="queue" class="com.gdoj.common.action.QueueAction">	
			<result name="success" >/jsp/error.jsp</result>
		</action>
